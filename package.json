{
    "name": "nestjs-coreui-starter",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": ">=21.0.0"
    },
    "scripts": {
        "build": "nest build",
        "build:all": "yarn build:scss && yarn build:webpack && yarn build:prisma && yarn build",
        "build:scss": "sass src-client/scss/style.scss dist/css/style.css --load-path=node_modules --style=compressed",
        "build:prisma": "prisma generate",
        "build:webpack": "webpack --mode=production",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\" \"*.js\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@coreui/coreui": "5.0.2",
        "@coreui/icons": "3.0.1",
        "@coreui/utils": "2.0.2",
        "@nestjs/class-validator": "0.13.4",
        "@nestjs/common": "10.3.10",
        "@nestjs/config": "3.2.3",
        "@nestjs/core": "10.3.10",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.3.10",
        "@nestjs/serve-static": "4.0.2",
        "@nestlab/google-recaptcha": "3.7.0",
        "@prisma/client": "5.16.2",
        "class-validator": "0.14.1",
        "connect-redis": "7.1.1",
        "cookie-parser": "1.4.6",
        "express": "4.19.2",
        "express-flash": "0.0.2",
        "express-handlebars": "7.1.3",
        "express-session": "1.18.0",
        "http-errors": "2.0.0",
        "luxon": "3.4.4",
        "passport": "0.7.0",
        "passport-local": "1.0.0",
        "redis": "4.6.15",
        "simplebar": "6.2.7",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.1"
    },
    "devDependencies": {
        "@babel/preset-env": "7.24.8",
        "@cloudflight/eslint-plugin-node": "0.26.0",
        "@nestjs/cli": "10.4.2",
        "@nestjs/schematics": "10.1.2",
        "@nestjs/testing": "10.3.10",
        "@types/cookie-parser": "1.4.7",
        "@types/express": "4.17.21",
        "@types/express-flash": "^0.0.5",
        "@types/express-session": "1.18.0",
        "@types/http-errors": "2.0.4",
        "@types/jest": "29.5.12",
        "@types/luxon": "^3.4.2",
        "@types/node": "20.14.10",
        "@types/passport": "1.0.16",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "7.16.0",
        "@typescript-eslint/parser": "7.16.0",
        "babel-loader": "9.1.3",
        "css-loader": "7.1.2",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "jest": "29.7.0",
        "prettier": "3.3.3",
        "prisma": "5.16.2",
        "sass": "1.77.8",
        "source-map-support": "0.5.21",
        "supertest": "6.3.4",
        "ts-jest": "29.2.2",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.5.3",
        "webpack": "5.93.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "5.0.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
